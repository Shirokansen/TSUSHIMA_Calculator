<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7C7C7C"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/button7"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="7"
        app:layout_constraintEnd_toEndOf="@+id/buttonC"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonC"
        app:layout_constraintTop_toBottomOf="@+id/buttonC" />

    <Button
        android:id="@+id/button4"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="4"
        app:layout_constraintEnd_toEndOf="@+id/button7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <Button
        android:id="@+id/buttondecimal"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="."
        app:layout_constraintEnd_toEndOf="@+id/button1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button1"
        app:layout_constraintTop_toBottomOf="@+id/button1" />

    <Button
        android:id="@+id/button1"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="1"
        app:layout_constraintEnd_toEndOf="@+id/button4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/buttonC"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="#FF7777"
        android:fontFamily="sans-serif-light"
        android:text="C"
        app:layout_constraintStart_toStartOf="@+id/button48"
        app:layout_constraintTop_toBottomOf="@+id/button48" />

    <Button
        android:id="@+id/buttonCE"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:backgroundTint="#FF8E8E"
        android:fontFamily="sans-serif-light"
        android:text="CE"
        app:layout_constraintStart_toEndOf="@+id/buttonC"
        app:layout_constraintTop_toTopOf="@+id/buttonC" />

    <Button
        android:id="@+id/buttonMODULO"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginStart="12dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="%"
        app:layout_constraintStart_toEndOf="@+id/buttonCE"
        app:layout_constraintTop_toTopOf="@+id/buttonC" />

    <Button
        android:id="@+id/button9"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="9"
        app:layout_constraintEnd_toEndOf="@+id/buttonMODULO"
        app:layout_constraintStart_toStartOf="@+id/buttonMODULO"
        app:layout_constraintTop_toBottomOf="@+id/buttonMODULO" />

    <Button
        android:id="@+id/buttonequals"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FF9F9F"
        android:fontFamily="sans-serif-light"
        android:text="="
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/buttonplus"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonplus"
        app:layout_constraintTop_toBottomOf="@+id/buttonplus" />

    <Button
        android:id="@+id/button8"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="8"
        app:layout_constraintEnd_toEndOf="@+id/buttonCE"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonCE"
        app:layout_constraintTop_toBottomOf="@+id/buttonCE" />

    <Button
        android:id="@+id/button2"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="2"
        app:layout_constraintEnd_toEndOf="@+id/button5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/button5"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="5"
        app:layout_constraintEnd_toEndOf="@+id/button8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button0"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="0"
        app:layout_constraintEnd_toEndOf="@+id/button2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/buttonDEL"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="DEL"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintStart_toStartOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button3"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="3"
        app:layout_constraintEnd_toEndOf="@+id/button6"
        app:layout_constraintStart_toStartOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button6"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:backgroundTint="#80AEFF"
        android:fontFamily="sans-serif-light"
        android:text="6"
        app:layout_constraintEnd_toEndOf="@+id/button9"
        app:layout_constraintStart_toStartOf="@+id/button9"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <Button
        android:id="@+id/buttonmultiply"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:backgroundTint="#84C5B8"
        android:fontFamily="sans-serif-light"
        android:text="X"
        app:layout_constraintEnd_toEndOf="@+id/buttondivide"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttondivide"
        app:layout_constraintTop_toBottomOf="@+id/buttondivide" />

    <Button
        android:id="@+id/buttondivide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:backgroundTint="#FFBD8B"
        android:fontFamily="sans-serif-light"
        android:text="/"
        app:layout_constraintEnd_toEndOf="@+id/button48"
        app:layout_constraintTop_toBottomOf="@+id/button48" />

    <Button
        android:id="@+id/buttonminus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FCA9FF"
        android:fontFamily="sans-serif-light"
        android:text="-"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/buttonmultiply"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/buttonmultiply"
        app:layout_constraintTop_toBottomOf="@+id/buttonmultiply" />

    <Button
        android:id="@+id/buttonplus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#9AEFA5"
        android:fontFamily="sans-serif-light"
        android:text="+"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/buttonminus"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonminus"
        app:layout_constraintTop_toBottomOf="@+id/buttonminus" />

    <TextView
        android:id="@+id/button48"
        android:layout_width="357dp"
        android:layout_height="190dp"
        android:layout_marginTop="64dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFD6B7"
        android:text="0"
        android:textAlignment="textEnd"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="FlowCa"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/button48"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="98dp"
        android:layout_height="17dp"
        android:layout_marginTop="40dp"
        android:fontFamily="sans-serif-light"
        android:text="Kouji Tsushima "
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/button0"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttondecimal"
        app:layout_constraintTop_toBottomOf="@+id/buttondecimal" />

</androidx.constraintlayout.widget.ConstraintLayout>